  "keywords": [
    "next",
    "next.js",
    "react",
    "tailwind",
    "tailwindcss",
    "typescript",
    "Vinayak",
    "Vinayak Sharma",
    "itsvinayak"
  ],
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/mdx": "^2.0.10",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "next": "^14.0.4",
    "next-contentlayer": "^0.3.4",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "rehype-autolink-headings": "^7.1.0",
